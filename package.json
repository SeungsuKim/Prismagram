{
  "name": "Prismagram",
  "version": "1.0.0",
  "description": "Cloning Instagram with Express, Prisma, React, and React Native",
  "main": "index.js",
  "repository": "https://github.com/SeungsuKim/Prismagram",
  "author": "seungsukim <seungsu0407@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.8.0",
    "copy": "^0.3.2",
    "dotenv": "^7.0.0",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.31.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.8.0",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.11"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "npm run deploy && npm run generate",
    "dev": "nodemon --exec babel-node src/server.js --delay 1",
    "prebuild": "npm run prisma",
    "build": "babel src -d build",
    "postbuild": "cd src && copy ./api/**/*.graphql ../build/api/",
    "prestart": "npm run build",
    "start": "node build/server.js"
  }
}
